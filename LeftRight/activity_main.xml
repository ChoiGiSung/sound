<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    >
    <TableRow
        >
        <Button
            android:id="@+id/btn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 8kHz"
            />
        <CheckBox
            android:id="@+id/ch1"
            />
    </TableRow>
   <TableRow>
       <Button
           android:id="@+id/btn2"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:onClick="onClick"
           android:text=" 10kHz"
           />
       <CheckBox
           android:id="@+id/ch2"
           />
   </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 12kHz"
            />
        <CheckBox
            android:id="@+id/ch3"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 14.1kHz"
            />
        <CheckBox
            android:id="@+id/ch4"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 14.9kHz"
            />
        <CheckBox
            android:id="@+id/ch5"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 15.8kHz"
            />
        <CheckBox
            android:id="@+id/ch6"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 16.7kHz"
            />
        <CheckBox
            android:id="@+id/ch7"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text=" 17.7kHz"
            />
        <CheckBox
            android:id="@+id/ch8"/>
    </TableRow>
    <TableRow>
        <Button
            android:id="@+id/btn_Go"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClick"
            android:text="테스트 하러 가기"/>
    </TableRow>

</TableLayout>